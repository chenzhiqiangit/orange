<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.orange.entity.UserBo">
	<select id="selectById" parameterType="java.lang.String" resultType="com.orange.entity.UserBo">
		select * from sys_user where user_id=#{userId}
	</select>
	
	<select id="selectAll"  resultType="com.orange.entity.UserBo">
		select * from sys_user limit 20;
	</select>
</mapper>